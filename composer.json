{
	"name": "sypets/sypets-example-slug",
	"type": "typo3-cms-extension",
	"description": "TYPO3 example for reproducing a TYPO3 issue with slugs",
	"autoload": {
		"psr-4": {
			"Sypets\\SypetsExampleSlug\\": "Classes/"
		}
	},
	"authors": [
		{
			"name": "Sybille Peters",
			"email": "sypets@gmx.de",
			"role": "Developer"
		}
	],
	"require": {
		"typo3/cms-core": "^11.5 || 12 || dev-main"
	},
	"license": "GPL-2.0-or-later",
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web",
			"extension-key": "sypets_example_slug"
		}
	}
}
